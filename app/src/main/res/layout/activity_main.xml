<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/pers"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="73dp"
        android:layout_marginEnd="29dp"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/menu"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="73dp"
        android:layout_marginStart="29dp"
        app:layout_constraintStart_toStartOf="parent"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textStyle="bold"
        android:text="Rent a car"
        android:textColor="@color/black"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"

        android:layout_marginTop="122dp"
        android:layout_marginStart="50dp"
        android:textSize="40sp"/>


    <EditText
        android:id="@+id/edittext"
        android:layout_width="309dp"
        android:layout_height="wrap_content"

        android:layout_marginTop="198dp"
        android:background="@drawable/btn"
        android:drawableStart="@drawable/magnify"
        android:drawablePadding="10dp"
        android:minHeight="48dp"
        android:text="Search"
        android:layout_marginStart="25dp"
        android:layout_marginEnd="25dp"
        android:textColor="@color/grey"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.cardview.widget.CardView
        android:id="@+id/button"
        android:layout_width="309dp"
        android:layout_height="120dp"
        android:backgroundTint="@color/dark_blue"
        app:cardCornerRadius="30dp"
        android:layout_marginStart="25dp"
        android:layout_marginEnd="26dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="281dp"
        />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="182dp"
        android:elevation="2dp"
        android:src="@drawable/group__4_"
        android:layout_marginBottom="9dp"
        app:layout_constraintBottom_toBottomOf="@+id/button"
        app:layout_constraintStart_toStartOf="@id/button"
        app:layout_constraintTop_toTopOf="@id/button" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bogamy"
        android:textStyle="bold"
        android:layout_marginStart="15dp"
        android:layout_marginBottom="15dp"
        android:textSize="22sp"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="@id/button"
        app:layout_constraintStart_toStartOf="@id/button"
        android:elevation="2dp"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:src="@drawable/str"
        android:layout_marginTop="53dp"
        app:layout_constraintStart_toStartOf="@id/button"
        android:elevation="2dp"
        app:layout_constraintTop_toTopOf="@id/button"/>


    <androidx.cardview.widget.CardView
        android:id="@+id/cardView2"
        android:layout_width="309dp"
        android:layout_height="120dp"
        android:backgroundTint="@color/pink"
        app:cardCornerRadius="30dp"
        app:layout_constraintTop_toBottomOf="@id/button"
        app:layout_constraintLeft_toLeftOf="@id/button"
        app:layout_constraintRight_toRightOf="@id/button"
        android:layout_marginTop="35dp"
        />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="186dp"
        android:elevation="2dp"
        android:src="@drawable/car"
        android:layout_marginTop="31dp"
        android:layout_marginBottom="26dp"
        app:layout_constraintBottom_toBottomOf="@+id/cardView2"
        app:layout_constraintStart_toStartOf="@id/cardView2"
        app:layout_constraintTop_toTopOf="@id/cardView2" />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:src="@drawable/str"
        app:layout_constraintStart_toStartOf="@id/cardView2"
        app:layout_constraintTop_toTopOf="@id/cardView2"
        android:layout_marginTop="53dp"
        android:elevation="2dp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bogamy"
        android:textStyle="bold"
        android:layout_marginStart="15dp"
        android:layout_marginBottom="15dp"
        android:textSize="22sp"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="@id/cardView2"
        app:layout_constraintStart_toStartOf="@id/cardView2"
        android:elevation="2dp"/>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView3"
        android:layout_width="309dp"
        android:layout_height="120dp"
        android:backgroundTint="@color/lightBlue"
        app:cardCornerRadius="30dp"
        app:layout_constraintTop_toBottomOf="@id/cardView2"
        app:layout_constraintLeft_toLeftOf="@id/cardView2"
        app:layout_constraintRight_toRightOf="@id/cardView2"
        android:layout_marginTop="35dp"
        />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:src="@drawable/str"
        app:layout_constraintStart_toStartOf="@id/cardView3"
        app:layout_constraintTop_toTopOf="@id/cardView3"
        android:layout_marginTop="53dp"
        android:elevation="2dp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bogamy"
        android:textStyle="bold"
        android:layout_marginStart="15dp"
        android:layout_marginBottom="15dp"
        android:textSize="22sp"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="@id/cardView3"
        app:layout_constraintStart_toStartOf="@id/cardView3"
        android:elevation="2dp"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="178dp"
        android:elevation="2dp"
        android:src="@drawable/groupbs"

        app:layout_constraintBottom_toBottomOf="@+id/cardView3"
        app:layout_constraintStart_toStartOf="@id/cardView3"
        app:layout_constraintTop_toTopOf="@id/cardView3" />


</androidx.constraintlayout.widget.ConstraintLayout>